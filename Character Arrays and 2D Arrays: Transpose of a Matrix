void transpose(int row,int col,int input[][1001]) {
    
     for (int j = 0; j < col; j++) {
        for (int i = 0; i < row; i++) {
            cout << input[i][j] << " ";
        }
        cout << endl;
    }
}



#include<iostream>
using namespace std;

#include "solution.h"
int main(){
    int row,col;
    cin>>row>>col;
    int input[row][1001];

    for(int i = 0;i<row;i++){
        for(int j = 0;j<col;j++){
            
            cin>>input[i][j];
        }
    }
    transpose(row,col,input);
}
