#include<iostream>
using namespace std;

int main() {
	// Write your code here
	int a;
	int b;
	int c;
	cin>>a;
	cin>>b;
	cin>>c;

	if(a+b>c && b+c>a && c+a>b ){

		if(a ==b && b==c && a==c)
		{
			cout<<"Equilateral Triangle";
		}
		else if ( a==b || b==c || a==c)
		{
			cout<<"Isosceles Triangle";
		}
		else
		{
			cout<<"Scalene Triangle";
		}
	}
	else{
		cout<<"Not a Triangle";
	}

}
