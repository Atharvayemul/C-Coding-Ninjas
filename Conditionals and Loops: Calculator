#include <iostream>
using namespace std;

int main() {
    while (true) {
        int choice;
        cin >> choice;

        switch (choice) {
            case 1: {
                int num1, num2;
                cin >> num1 >> num2;
                cout << num1 + num2 << endl;
                break;
            }
            case 2: {
                int num1, num2;
                cin >> num1 >> num2;
                cout << num1 - num2 << endl;
                break;
            }
            case 3: {
                int num1, num2;
                cin >> num1 >> num2;
                cout << num1 * num2 << endl;
                break;
            }
            case 4: {
                int num1, num2;
                cin >> num1 >> num2;
                if (num2 != 0) {
                    cout << num1 / num2 << endl;
                } else {
                    cout << "Division by zero is not allowed" << endl;
                }
                break;
            }
            case 5: {
                int num1, num2;
                cin >> num1 >> num2;
                if (num2 != 0) {
                    cout << num1 % num2 << endl;
                } else {
                    cout << "Division by zero is not allowed" << endl;
                }
                break;
            }
            case 6: {
                return 0;  // Exit the program
            }
            default:
                cout << "Invalid Operation" << endl;
        }
    }

    return 0;
}
