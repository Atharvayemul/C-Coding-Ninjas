#include <iostream>
using namespace std;

int main() {
    int N;
    cin >> N;

    // Loop through rows from 1 to N
    for (int i = 1; i <= N; i++) {
        // Print numbers in reverse order from i to 1, separated by ''
        for (int j = i; j >= 1; j--) {
            cout << j;
        }
        // Move to the next line after each row
        cout << endl;
    }

    return 0;
}
