#include<iostream>
using namespace std;

int main(){
    /* Read input as specified in the question.
     * Print output as specified in the question.
     */
     int arr[100000];
     int n ;
     cin >> n;

     for (int i = 0; i < n; i++)
     {
         cin>> arr[i];
     }

     int sum = 0;

     for(int j = 0 ; j < n ; j++)
     {
         sum+=arr[j];
     }
    
    cout<<sum;
}


